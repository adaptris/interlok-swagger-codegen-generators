        "summary": "Get a contact address by contactId and addressName",
        "parameters": [
          {
            "name": "contactId",
            "in": "path",
            "description": "The ID that needs to be fetched.",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "addressName",
            "in": "path",
            "description": "The Name of the address that needs to be fetched.",
            "required": true,
            "schema": {

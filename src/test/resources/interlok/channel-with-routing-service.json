        "summary": "Get a contact by contactId",
        "parameters": [
          {
            "name": "contactId",
            "in": "path",
            "description": "The ID that needs to be fetched.",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
